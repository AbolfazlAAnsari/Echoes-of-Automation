                {
                    "generated_prob": 0.0009260434890165925,
                    "sentence": "We've got issues.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00107343471609056,
                    "sentence": "Remember when summer",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006878876010887325,
                    "sentence": "Was sunshine and crickets?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0008269737008959055,
                    "sentence": "Kids in the street",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006140161422081292,
                    "sentence": "And fences in pickets?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.000811904901638627,
                    "sentence": "Today many days",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.000976424606051296,
                    "sentence": "Are powered by tools",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0008356390753760934,
                    "sentence": "We'd rather be lounging",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006673932657577097,
                    "sentence": "With picnics at pools.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0008228629594668746,
                    "sentence": "Hackers and whackers",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0007184991845861077,
                    "sentence": "And trimmers and mowers",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006862843874841928,
                    "sentence": "Loudest of all",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0013460386544466019,
                    "sentence": "Are gas-powered blowers.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001002206583507359,
                    "sentence": "They blow blades of grass,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0007647150196135044,
                    "sentence": "A handful of leaves,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0015630213310942054,
                    "sentence": "A big cloud of dust",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001245464663952589,
                    "sentence": "Like you wouldn't believe.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006972062401473522,
                    "sentence": "In an hour, it's back,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0010018555913120508,
                    "sentence": "The dust and the leaves",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001419384265318513,
                    "sentence": "That need nothing more",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0009488882496953011,
                    "sentence": "Than the tiniest breeze.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0011673846747726202,
                    "sentence": "Thirty minutes of blowing",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0012212253641337156,
                    "sentence": "Is worse than a truck",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0012746206484735012,
                    "sentence": "Monoxide and benzene",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0012177209137007594,
                    "sentence": "From dawn until dusk.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0013412826228886843,
                    "sentence": "Exhaust that comes out",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0014216258423402905,
                    "sentence": "From the tank and the tube",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0012831139611080289,
                    "sentence": "Is bad for your neighbor",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0012131412513554096,
                    "sentence": "And so bad for you.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0007811193354427814,
                    "sentence": "Summer's upon us",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0016481849597766995,
                    "sentence": "The best season of all",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0010328794596716762,
                    "sentence": "Blue skies and a breeze",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0012722298270091414,
                    "sentence": "Perfection til fall.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0010245871962979436,
                    "sentence": "Can we think of a way",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0016374137485399842,
                    "sentence": "To get the job done?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0009142033522948623,
                    "sentence": "If you need it, electric,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001627223682589829,
                    "sentence": "But a broom is more fun!",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001262826845049858,
                    "sentence": "The gas-powered leaf blower spews hazardous levels of carbon monoxide, nitrous oxide, hydrocarbons and other cancer-causing contaminants into our summer air.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0016017367597669363,
                    "sentence": "The toxic emissions come in large quantities and are dangerous to anyone who breathes.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001624703872948885,
                    "sentence": "The gas blower is the biggest hazard to the user, who is just inches away from unfiltered exhaust and extreme noise.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0021358372177928686,
                    "sentence": "Several cities restrict or ban the gas-powered blower.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0023279287852346897,
                    "sentence": "Sources from Washington University in St. Louis, to New York State Department of Environmental Conservation, to the Centers for Disease Control and Prevention decry the hazards of gas blowers.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.07352695614099503,
                    "sentence": "One hour of the best-selling commercial leaf blower emits as much smog-forming pollution as driving a 2016 Toyota Camry 1,100 miles, according to CalEPA.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0938577651977539,
                    "sentence": "Leaf blowers, lawn mowers and other non vehicular engines are not regulated as car and truck engines are, and predictions suggest smog from these popular lawn tools with two-stroke engines will eclipse vehicle smog emissions within a couple years.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0515250563621521,
                    "sentence": "Leaf blowers push up to 700 cubic feet of air per minute at up to 200 mph, stirring up fine-particle hurricanes that include pollen, mold, animal feces, heavy metals, herbicides and pesticides, causing damage to the respiratory system.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.05260405316948891,
                    "sentence": "Where does the dust go that the landscaper blows off a driveway and across the street?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.043164923787117004,
                    "sentence": "The homeowner's dust is now everyone's dust.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.05570178106427193,
                    "sentence": "It's like someone smoking in your kitchen, and you can't get away.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.05056752264499664,
                    "sentence": "Gas blower noise is so loud it jars the mind and makes your teeth rattle, launching you in search of quiet that can't be found.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.05934382602572441,
                    "sentence": "The noise is not only obnoxious, but it's also harmful to our health and a quick cause of hearing loss to users.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.08311694115400314,
                    "sentence": "Blower noise can be heard five blocks away, even farther when it carries across a canyon.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.1181953176856041,
                    "sentence": "Blowers tend to ruin a summer evening.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.044970594346523285,
                    "sentence": "Or morning.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.08627881109714508,
                    "sentence": "In my neighborhood, four gas-powered tools fire up at 7:30 am on Tuesday.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.06673665344715118,
                    "sentence": "Another four at 6:30 pm on Wednesday.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.08860720694065094,
                    "sentence": "Four houses away, a team comes every Friday morning, another up the hill Friday afternoon, and another, long and loud, comes four houses the other direction on random days.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004143234109506011,
                    "sentence": "By February, the landscape service, in search of a good reason for the weekly visit, continues to fire up two gas blowers ᅳ one to blow a leaf up the driveway and one to blow water down.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0005132892401888967,
                    "sentence": "Thirty minutes later, blowers and mowers pack up and drive away.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00048447016160935163,
                    "sentence": "Sixty minutes later, the wind picks up and returns the leaf to the driveway.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0005032792687416077,
                    "sentence": "Next morning, a gentle rain leaves more water.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00042425948777236044,
                    "sentence": "Homeowners believe the landscaper insists upon a gas blower, mower or weed whacker.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.000384387094527483,
                    "sentence": "The landscaper believes the homeowner insists upon the same.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00042115477845072746,
                    "sentence": "Maybe not.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0002888377057388425,
                    "sentence": "Maybe an alternative is possible.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0003235487383790314,
                    "sentence": "Many city dwellers have tiny yards, with an occasional need for heavy-duty tools.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00026079421513713896,
                    "sentence": "What did we do before the advent of noise- and air-polluting blowers and whackers?We hired three strong men to help maintain the yard.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0003741373075172305,
                    "sentence": "Today, three strong men or women could be as speedy with a rake, broom or push mower as they were in 1980.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00040070206159725785,
                    "sentence": "Remember the sweet sounds of summer?",
                    "perplexity": 0,
