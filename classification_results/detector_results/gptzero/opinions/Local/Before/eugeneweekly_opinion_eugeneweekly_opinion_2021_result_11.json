                },
                {
                    "generated_prob": 0.00034741678973659873,
                    "sentence": "A powered-up blowing blower takes up airspace and absorbs the sweet sounds.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
