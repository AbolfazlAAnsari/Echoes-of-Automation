                },
                {
                    "generated_prob": 0.0004169595194980502,
                    "sentence": "You have to know this: All the killing has changed me.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004237427783664316,
                    "sentence": "Seeing black people die over and over and over again.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00044597749365493655,
                    "sentence": "Day after year, after dawn, after dusk, after vigil after rally, after riot, after hashtag, after prayers, after rage, after no sleep nights and someone do something, after white people and other people turning a blind ear.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006687005516141653,
                    "sentence": "The murder of black men and women has changed me.",
                    "perplexity": 0,
