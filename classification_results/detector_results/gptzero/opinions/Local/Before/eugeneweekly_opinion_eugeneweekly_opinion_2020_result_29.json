                },
                {
                    "generated_prob": 0.0004889768897555768,
                    "sentence": "Altered who I am in America.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00046438767458312213,
                    "sentence": "My burden is now, and has always been, yours.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004094394971616566,
                    "sentence": "You are not obligated to believe me.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0002533234655857086,
                    "sentence": "Just look around.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004509234568104148,
                    "sentence": "Witness what we have all become.",
                    "perplexity": 0,
