    "documents": [
        {
            "sentences": [
                {
                    "generated_prob": 0.000849823234602809,
                    "sentence": "We've got issues.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006637733895331621,
                    "sentence": "These words are only my own.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.000566990755032748,
                    "sentence": "The last time someone called me “nigger” out loud was 1976.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0005679114838130772,
                    "sentence": "It was broad daylight and I was a sophomore at Mountain Home High School in Mountain Home, Idaho.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006865527248010039,
                    "sentence": "There, the little white boys and girls who lived in town wore cowboy boots, drove trucks, spit tobacco juice in the drinking fountains and raised livestock as some sort of hobby.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0012153297429904342,
                    "sentence": "My family lived on the Air Force base near the town.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0011787002440541983,
                    "sentence": "Air Force kids came from everywhere, had lived everywhere.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006561409099958837,
                    "sentence": "We were safe for each other.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0008846499258652329,
                    "sentence": "The tobacco-spitters knew that we were destined to leave the dusty-podunk-nowhere town sitting in its own homegrown dust.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006949724047444761,
                    "sentence": "He was not bright at all.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0007508586859330535,
                    "sentence": "I helped him pass English.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00042324993410147727,
                    "sentence": "And when he said it, I didn't hear it.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00044259463902562857,
                    "sentence": "It registered as sound, as utterance, but not as an actual word.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006194608868099749,
                    "sentence": "Once it hit the air, it tore through my skull; lodged itself between my eyes.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004509145801421255,
                    "sentence": "Then, I understood that “nigger” was a weapon.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00039456304511986673,
                    "sentence": "Their weapon.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0005651586106978357,
                    "sentence": "White people's weapon.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0005263496423140168,
                    "sentence": "It made me undeniably black.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00031299592228606343,
                    "sentence": "Finally.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.000643897510599345,
                    "sentence": "Inside, outside, in my guts, in my brains.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0003086616052314639,
                    "sentence": "And although I will never forget that searing revelation of my black-blackness, I had never really thought that it would mark me as quarry in the same way it marked my slave ancestors.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0007691226783208549,
                    "sentence": "Then, one day a former cohort of mine at the University of Oregon casually asked me, “What is your personal politics of race?”",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001359543763101101,
                    "sentence": "What?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001228216104209423,
                    "sentence": "What the hell is that?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001600603573024273,
                    "sentence": "Does she not see my skin?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0014632195234298706,
                    "sentence": "Is this woman serious?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0018654953455552459,
                    "sentence": "Should I get one?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0019519546767696738,
                    "sentence": "(Oh, God!",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0019404107006266713,
                    "sentence": "I probably need to read more books.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001683902577497065,
                    "sentence": "Like, Judith Butler, maybe?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0017861281521618366,
                    "sentence": "Or… what was that other woman's name?)",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0018669796409085393,
                    "sentence": "Although I was stymied by her question back then, my understanding of it now is like smooth stone.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006340430700220168,
                    "sentence": "That textbook-based version of a personal politics of race is a luxury I cannot afford.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0010141516104340553,
                    "sentence": "My so-called personal politics of race is no longer just mine nor is it solely personal.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0008775891037657857,
                    "sentence": "It is no longer some gauzy, ethereal consideration blithely discussed by so-called progressives (tenured or not) at heady academic conferences where everyone is a doctor of something or other.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006698797224089503,
                    "sentence": "No.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0009053219691850245,
                    "sentence": "When Amadou Diallo (may the angels cradle him) was murdered during my first year of graduate school, the phrase personal politics of race came to mean keeping myself safe because my life could be stubbed out like a cigarette.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0014100794214755297,
                    "sentence": "God, rest your child, Sandra Bland.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0005831666057929397,
                    "sentence": "Or that a white someone, a white anyone, could shoot me dead in a well-lit parking lot or a dark cavernous alley for be-ing.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004820076865144074,
                    "sentence": "Black.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004729395150206983,
                    "sentence": "Woman.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004812557599507272,
                    "sentence": "Unarmed.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0012183551443740726,
                    "sentence": "Trayvon Martin was just a kid.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006684534600935876,
                    "sentence": "My personal politics of race means keeping my phone charged, camera ready in case I need or need to be a witness.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006575369625352323,
                    "sentence": "And I will not roll my window down without calling an ally and waiting for them to arrive if I am stopped by a police officer while in the car alone.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006962885381653905,
                    "sentence": "Not.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.000803616363555193,
                    "sentence": "Anymore.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0004750137450173497,
                    "sentence": "Personal politics of race means crossing to the other side of the street at the sight of two or three or five or 12 white, college-age men walking toward me on the sidewalk in order to stay alive.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00043237663339823484,
                    "sentence": "Yes.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00036486057797446847,
                    "sentence": "Yes, it does.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006701286765746772,
                    "sentence": "My inner universe howled when George Floyd was murdered by a white police officer who placed his knee on George Floyd's neck until George Floyd was lifeless.",
                    "perplexity": 0,
