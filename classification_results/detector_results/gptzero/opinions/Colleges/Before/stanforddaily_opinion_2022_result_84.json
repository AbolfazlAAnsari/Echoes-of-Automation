                },
                {
                    "generated_prob": 0.004567816387861967,
                    "sentence": "It's not that.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005540584214031696,
                    "sentence": "My sibling found shul comforting, but they did not go for five years.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0062152184545993805,
                    "sentence": "Religious life as a queer person is scary.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0060789757408201694,
                    "sentence": "Hebrew itself is a language with two genders ᅳ it does not accommodate for them.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005659324117004871,
                    "sentence": "I'm not angry at Hebrew for this.",
