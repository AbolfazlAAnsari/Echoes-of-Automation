                },
                {
                    "generated_prob": 0.004389821086078882,
                    "sentence": "So helpmeunderstand.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.004480978008359671,
                    "sentence": "Why does it matter if my sibling is not one of two genders?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.008020281791687012,
                    "sentence": "Please don't come to me with religion.",
