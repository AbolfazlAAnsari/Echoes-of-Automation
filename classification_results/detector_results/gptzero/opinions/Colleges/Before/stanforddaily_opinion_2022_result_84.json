            "sentences": [
                {
                    "generated_prob": 0.012119034305214882,
                    "sentence": "The author has requested anonymity for fear of harassment and doxxing.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.013826480135321617,
                    "sentence": "Content warning: this article contains references to suicide, self-harm and transphobia.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006364462431520224,
                    "sentence": "My sibling's mind had changed by the time they were 11.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.004347668029367924,
                    "sentence": "That was in 2015.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.01149773970246315,
                    "sentence": "Neither of us understood the concept of gender or nonbinary yet ᅳ or even what it really meant to be trans, for that matter.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005492562893778086,
                    "sentence": "But my sibling told me, “I don't think I can be a ___.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0067297727800905704,
                    "sentence": "But I know I'm not a ___, either.”",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.004231330938637257,
                    "sentence": "For a year after that, my sibling fought their gender.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006722640246152878,
                    "sentence": "They tried on different clothes and different pronouns.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00812268815934658,
                    "sentence": "None of it felt right.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005455542355775833,
                    "sentence": "Then, when they were 12, we were introduced to the concept of nonbinary.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005428745876997709,
                    "sentence": "I don't need you to understand what it means to be nonbinary.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.004202002659440041,
                    "sentence": "But can you understand the smile on my sibling's face?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006083510350435972,
                    "sentence": "The way their back straightened, the way their voice got stronger and clearer in an instant?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00500711053609848,
                    "sentence": "Life was not easy for them.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.004519585520029068,
                    "sentence": "This world is not tailored to trans people; this world beats them down and does its best to undermine their sense of self.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.003234834410250187,
                    "sentence": "Teachers at my sibling's middle school refused to call my sibling by their name or pronouns.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005063140764832497,
                    "sentence": "We were lucky enough to have supportive parents, who went to school administrators and asked that my sibling's chosen name be noted on official documents.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006294836290180683,
                    "sentence": "At their middle school, students' nicknames were listed alongside their given names.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.008104800246655941,
                    "sentence": "Teachers used all of these “nicknames” except for my sibling's.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006282390095293522,
                    "sentence": "When my sibling asked their history teacher why, the teacher said, “that is not your name.”",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.008088326081633568,
                    "sentence": "There were no non-gendered bathrooms at their middle school.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.010109975934028625,
                    "sentence": "Admin told them they would simply have to use the ___'s bathroom.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0062056900933384895,
                    "sentence": "My sibling did, once.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.010373225435614586,
                    "sentence": "They went back to class and smiled through it.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.008369016461074352,
                    "sentence": "Then they came home and cried.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006032992620021105,
                    "sentence": "They chanted, over and over, “I'm not a ___, I'm not a ___, I'm not a ____ ᅳ they're going to say that I'm a ___, they're going to say I did it once and I'm wrong, I'm lying, I'm a stupid lying ___, but I'mnotᅳ ”",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006986761465668678,
                    "sentence": "Our grandmother sent them emails with articles explaining to them why being trans and nonbinary was not real, and how they would grow out of it and “be normal” again in a few years.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006745774298906326,
                    "sentence": "Some of their friends ceased being their friends.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00897971447557211,
                    "sentence": "Some of their friends were told by their parents to stop spending time with my sibling.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.008513684384524822,
                    "sentence": "My sibling's pronouns were contagious.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.007094849366694689,
                    "sentence": "My sibling's identity was dangerous.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005312388762831688,
                    "sentence": "Their existence was disgusting.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005154492799192667,
                    "sentence": "Their existence was not allowed.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0062128459103405476,
                    "sentence": "Their existence was debated, again and again, in front of them, on national television in political and pop cultural debates, in the hallways where they were told over and over that they were wrong.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.004266491625458002,
                    "sentence": "When my sibling was 13 they were hospitalized for a suicide attempt for the first time.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.002640846651047468,
                    "sentence": "Thirteen.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.005661346949636936,
                    "sentence": "13. 10 + 3 years.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.006271415390074253,
                    "sentence": "Do you know what that means?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.008718072436749935,
                    "sentence": "Do you understand that my sibling was in so much pain that when they were 13 years old, barely in puberty, they attempted to end their life?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.007684648968279362,
                    "sentence": "Are you a big sister?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00979615654796362,
                    "sentence": "Are you a big sibling?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.008213046938180923,
                    "sentence": "Do you understand what that means?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.01637612096965313,
                    "sentence": "To come home and be told they are in the hospital?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.013175173662602901,
                    "sentence": "They had wanted to not be alive.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.01769328862428665,
                    "sentence": "They were never going to smile or laugh or annoy me again.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.018472420051693916,
                    "sentence": "Do you know what it is to be terrified?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.007223057560622692,
                    "sentence": "Guilty?",
                    "perplexity": 0,
