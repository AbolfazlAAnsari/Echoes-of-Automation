                },
                {
                    "generated_prob": 0.015442756935954094,
                    "sentence": "Do you know the shame?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.011150429025292397,
                    "sentence": "The rage?",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.015003807842731476,
                    "sentence": "They would never smile or laugh again, but at least they would never have to experience being told that their identity was not real, that something as tangible and true as the pavement under our feet was a lie.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.02135019190609455,
                    "sentence": "My sibling was in and out of the hospital for years.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.007053222972899675,
                    "sentence": "My baby sibling wanted to die for years.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.004951515235006809,
                    "sentence": "Do you understand that?",
                    "perplexity": 0,
